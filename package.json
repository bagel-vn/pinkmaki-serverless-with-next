{
  "name": "pinkmaki-serverless-with-next",
  "version": "1.0.0",
  "description": "Serverless with Next",
  "scripts": {
    "start": "concurrently -p '{name}' -n 'next,sls' -c 'gray.bgWhite,yellow.bgBlue' \"next -p 3001\" \"serverless offline start\"",
    "next": "next",
    "next:build": "next build",
    "build": "cross-env-shell NODE_ENV=development \"next build && serverless package --stage development\"",
    "build:fast": "cross-env-shell NODE_ENV=staging \"serverless package --stage staging\"",
    "build:production": "cross-env-shell NODE_ENV=production \"next build && serverless package --stage production\"",
    "emulate": "cross-env-shell NODE_ENV=staging \"next build && serverless offline\"",
    "emulate:production": "cross-env-shell NODE_ENV=production \"next build && serverless offline\"",
    "deploy": "cross-env-shell NODE_ENV=development \"npm run build && serverless deploy -v --package .serverless --stage development\"",
    "deploy:fast": "cross-env-shell NODE_ENV=staging \"npm run build:fast && serverless deploy -v --package .serverless --stage staging\"",
    "deploy:production": "cross-env-shell NODE_ENV=production \"npm run build:production && serverless deploy -v --package .serverless --stage production\"",
    "test:create": "sls create test --path {function}",
    "test": "jest"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-module-resolver": "3.1.0",
    "babel-plugin-source-map-support": "2.0.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "concurrently": "3.5.1",
    "cross-env": "5.1.3",
    "copy-webpack-plugin": "4.4.2",
    "jest": "22.4.2",
    "serverless-domain-manager": "2.3.3",
    "serverless-jest-plugin": "0.1.6",
    "serverless-offline": "3.18.0",
    "serverless-webpack": "4.3.0",
    "webpack": "3.11.0",
    "webpack-node-externals": "1.6.0"
  },
  "dependencies": {
    "aws-sdk": "2.194.0",
    "aws-serverless-express": "3.1.3",
    "babel-runtime": "6.26.0",
    "compression": "1.7.2",
    "express": "4.16.2",
    "express-http-proxy": "1.1.0",
    "isomorphic-fetch": "2.2.1",
    "lodash.get": "4.4.2",
    "moment": "2.21.0",
    "next": "5.0.1-canary.9",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "source-map-support": "0.5.3",
    "stacktrace-js": "2.0.0",
    "url": "0.11.0"
  },
  "keywords": [],
  "author": "Vadorequest (http://github.com/vadorequest)",
  "license": "MIT"
}
